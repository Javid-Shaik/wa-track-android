<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- When the chip is selected or checked, use @color/chip_text_selected_color -->
    <item android:color="@color/chip_text_selected_color" android:state_selected="true" />
    <item android:color="@color/chip_text_selected_color" android:state_checked="true" />
    <!-- Default text color is @color/textSecondary -->
    <item android:color="@color/textSecondary" />
</selector>